<p id="notice"><%= notice %></p>

<h1>Contractees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Pass active</th>
      <th>Acc active</th>
      <th>Last in</th>
      <th>Password digest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contractees.each do |contractee| %>
      <tr>
        <td><%= contractee.name %></td>
        <td><%= contractee.email %></td>
        <td><%= contractee.pass_active %></td>
        <td><%= contractee.acc_active %></td>
        <td><%= contractee.last_in %></td>
        <td><%= contractee.password_digest %></td>
        <td><%= link_to 'Show', contractee %></td>
        <td><%= link_to 'Edit', edit_contractee_path(contractee) %></td>
        <td><%= link_to 'Destroy', contractee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contractee', new_contractee_path %>
